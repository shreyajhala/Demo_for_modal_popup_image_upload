Welcome back, <%= current_user.email %><br />
<%= link_to('Logout', destroy_user_session_path, :method => :delete, :class=> "btn btn-primary") %>
<button class="btn btn-primary btn-lg" data-toggle="modal" data-target="#myModal">
  Upload Image
</button>

<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Upload Pics</h4>
      </div>
      <div class="modal-body">
      Hello
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" id="close">Cancel</button>
        </div>
    </div>
  </div>
</div>       

<div id="my_file_uploader"></div>
<script type="text/javascript">
    var uploader = new qq.FileUploader({
        element: document.getElementById('my_file_uploader'),
        action: '<%= add_files_photo_index_path %>',
        params: { "authenticity_token": "<%= form_authenticity_token %>" },
        customHeaders: { "X-File-Upload": "true" }
    });
</script>